{
  "title": "前端PC框架",
  "name": "front-template-pc-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "yarn dev --host",
    "dev": "cross-env VITE_RUN_ENV=development vite",
    "build": "tsc -b && cross-env VITE_RUN_ENV=production vite build",
    "build:test": "tsc -b && cross-env VITE_RUN_ENV=test vite build",
    "build:preview": "tsc -b && cross-env VITE_RUN_ENV=preview vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "yarn build && cp -r build/* /root/nginx/html/car-source"
  },
  "dependencies": {
    "antd-mobile": "^5.41.1",
    "axios": "^1.13.1",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "path-to-regexp": "^8.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.7.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@antfu/utils": "^9.2.0",
    "@eslint/js": "^9.30.1",
    "@types/lodash": "^4.17.20",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.0.15",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "less": "^4.4.0",
    "micromatch": "^4.0.8",
    "picocolors": "^1.1.1",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.1.12",
    "vite-plugin-eslint": "^1.8.1"
  }
}
